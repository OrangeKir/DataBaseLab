<UserControl x:Class="DataBaseLab.StructureView.View_Elements.AgentStruct"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:DataBaseLab.StructureView.View_Elements"
             mc:Ignorable="d" 
             Height="50" d:DesignWidth="650"
             Background="LightGray">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="70"/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Rectangle Grid.Row="0" Fill="DarkGray"/>
        <TextBlock x:Name="IdText" HorizontalAlignment="Center" VerticalAlignment="Center" TextWrapping="Wrap" Text="Id"/>
        <TextBlock x:Name="NameText" Grid.Column="1" HorizontalAlignment="Center" VerticalAlignment="Center" TextWrapping="Wrap" Text="Conteragent Name"/>
        

    </Grid>
</UserControl>
