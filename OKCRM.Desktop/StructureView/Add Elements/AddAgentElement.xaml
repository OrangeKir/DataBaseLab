<UserControl x:Class="DataBaseLab.StructureView.Add_Elements.AddAgentElement"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:DataBaseLab.StructureView.Add_Elements"
             mc:Ignorable="d" 
             d:DesignHeight="50" d:DesignWidth="650"
             Background="LightGray">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="70"/>
            <ColumnDefinition/>
            <ColumnDefinition Width="17"/>
        </Grid.ColumnDefinitions>
        <Rectangle Grid.Row="0" Fill="DarkGray"/>
        <TextBlock x:Name="IdText" HorizontalAlignment="Center" VerticalAlignment="Center" TextWrapping="Wrap" Text="Id" Foreground="Gray" Margin="30,17,29,17"/>
        <TextBox x:Name="NameText" Grid.Column="1" HorizontalAlignment="Center" VerticalAlignment="Center" TextWrapping="Wrap" Text="Conteragent Name" Background="LightGray"/>


    </Grid>
</UserControl>
